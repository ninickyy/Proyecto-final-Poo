package modelo;

public ReporteElectoral {

    public void generarReportePorMesa(MesaElectoral m) {
        System.out.println("=== REPORTE POR MESA ===");
        System.out.println(m.GenerarInforme());
    }

    public void generarReportePorEleccion(Elecciones e) {
        System.out.println("=== REPORTE POR ELECCION ===");
        System.out.println(e.GenerarInforme());
    }

    public void generarReportePorActa(ActaElectoral a) {
        System.out.println("=== REPORTE POR ACTA ===");
        System.out.println(a.GenerarInforme());
    }
}
